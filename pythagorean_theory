import math

a = float(input("Enter value for a: "))
b = float(input("Enter value for b: "))
c_is_greater_than_a = input("Is c greater than a? (T/F): ").strip().lower()
if c_is_greater_than_a == "t":
    c = math.sqrt(a**2+b**2)
else:
    if a > b:
        c = math.sqrt(a**2-b**2)
    else:
        c = math.sqrt(b**2-a**2)
print(f"The value of c is: {c}")
